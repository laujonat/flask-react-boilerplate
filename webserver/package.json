{
  "name": "webserver",
  "version": "1.0.0",
  "description": "flask express ssr",
  "main": "./server/index.ts",
  "dependencies": {
    "@types/react": "^16.8.20",
    "express": "^4.17.1",
    "react": "^16.8.6",
    "tslib": "^1.10.0"
  },
  "peerDependencies": {
    "@babel/core": "@^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.8",
    "@types/react-dom": "^16.8.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "debug": "npm run build -- --progress --colors --profile --display-modules --display-reasons --display-error-details",
    "build": "npm run all -s clean && lint && build:tsc",
    "clean": "rimraf ./build && mkdir ./build",
    "lint": "tslint -c tslint.json \"client/**/*.ts\" \"server/**/*.ts\"",
    "lint:fix": "tslint --fix -c tslint.json \"client/**/*.ts\" \"server/**/*.ts\"",
    "start": "npm install --only=prod && NODE_ENV=production node ./server/index.ts",
    "start:dev": "npm run build && nodemon --config \"./nodemon.json\" build/**/*",
    "serve": "node build/js/server.js",
    "build:tsc": "tsc",
    "watch:ts": "nodemon --config \"./nodemon.json\" build/**/*"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "keywords": [
    "flask",
    "ssr",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
